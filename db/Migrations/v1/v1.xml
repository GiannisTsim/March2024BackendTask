<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <include file="table.sql" relativeToChangelogFile="true"/>
    <include file="view.sql" relativeToChangelogFile="true"/>
    <include file="message.sql" relativeToChangelogFile="true"/>
    <includeAll path="transaction" relativeToChangelogFile="true"/>
    <include file="test.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>
